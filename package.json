{
  "name": "fdis-node-orm",
  "author": "phuong.tx",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon src/index.js --exec \"LOCAL_MODE=true node -r dotenv/config -r babel-register\"",
    "start": "node src/index.js --exec \"NODE_ENV=production node -r dotenv/config -r babel-register\"",
    "queue": "node src/moduls/index.js --exec \"NODE_ENV=production node -r dotenv/config -r babel-register\"",
    "mocha": "nyc mocha src/test -r dotenv/config -r babel-register --timeout=30000 --exit",
    "test": "npm run mocha",
    "lint": "eslint --ext .js src",
    "precommit": "npm run lint",
    "precommit-msg": "echo 'Pre-commit: checking for coding conventions...' && exit 0",
    "apidoc": "apidoc -i src/packages/ -o apidoc/"
  },
  "husky": {
    "hooks": {
      "pre-commit": [
        "precommit-msg",
        "precommit"
      ],
      "pre-push": ""
    }
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "nodemonConfig": {
    "verbose": false,
    "ignore": [
      "node_modules",
      "src/files",
      "logs"
    ],
    "ext": "js"
  },
  "dependencies": {
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@hapi/joi": "^15.1.1",
    "@sendgrid/mail": "^7.7.0",
    "aws-sdk": "^2.1449.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "cluster": "^0.7.7",
    "commander": "^7.2.0",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.4",
    "cron": "^1.3.0",
    "crypto-js": "^4.0.0",
    "crypto-random-string": "^3.1.0",
    "dateformat": "^3.0.3",
    "debug": "~2.6.9",
    "dotenv": "^5.0.1",
    "express": "^4.18.2",
    "express-validation": "^1.0.2",
    "helmet": "^3.12.0",
    "http-errors": "~1.6.2",
    "i18n": "^0.8.4",
    "ioredis": "^4.19.4",
    "ip": "^1.1.5",
    "joi": "^13.7.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "method-override": "^3.0.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.43",
    "morgan": "~1.9.0",
    "mssql": "^7.2.1",
    "multer": "^1.4.4",
    "multer-s3": "^3.0.1",
    "multer-s3-transform": "^2.3.2",
    "redis": "^3.1.2",
    "request": "^2.88.0",
    "sequelize": "^6.32.1",
    "sharp": "^0.28.2",
    "tedious": "^14.0.0",
    "uuid": "^9.0.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "faker": "^4.1.0",
    "husky": "^6.0.0",
    "mocha": "^9.0.0",
    "nodemon": "^2.0.22",
    "nyc": "^15.1.0",
    "pre-commit": "^1.2.2"
  }
}
